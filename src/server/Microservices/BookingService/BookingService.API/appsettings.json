{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "Port": 7003,
    "UsersPort": 7001,
    "MoviesPort": 7002,
    "LogstashPort": 5044
